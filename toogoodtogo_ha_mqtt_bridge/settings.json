{
  "mqtt": {
    "client": "toogoodtogo-ha-mqtt-bridge",
    "base": "toogoodtogo",
    "raw": false
  },
  "homeassistant": {
    "enabled": true,
    "base": "homeassistant",
    "name_prefix": "TooGoodToGo - "
  },
  "timezone": "Europe/Berlin",
  "locale": "en_us",
  "cleanup": true,
  "data_dir": "./data",
  "randomize_calls": true,
  "enable_auto_intense_fetch": false
}
